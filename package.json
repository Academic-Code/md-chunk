{
  "name": "flow-scc",
  "version": "0.1.0",
  "main": "index.js",
  "description": "About Creator of statistical content, your cms in cli format and generation of html files and statistical markdown.",
  "repository": "git@github.com:edmilson-dk/flow-scc.git",
  "author": "Edmilson Jesus <droidvidaboa@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start:dev": "npx ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format",
    "postversion": "git push && git push --tags"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^16.3.1",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "HTML",
    "Markdown",
    "CLI"
  ]
}
